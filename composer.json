{
    "name": "tribe/bigcommerce-api",
    "description": "Collection of Big Commerce API clients in php.",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://swagger.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Julian Wood",
            "homepage": "https://tri.be"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": {
          "BigCommerce\\Widgets\\" : "./clients/Widgets/SwaggerClient-php/lib/",
          "BigCommerce\\Themes\\" : "./clients/Themes/SwaggerClient-php/lib/",
          "BigCommerce\\Catalog\\" : "./clients/Catalog/SwaggerClient-php/lib/",
          "BigCommerce\\OrdersV2\\" : "./clients/OrdersV2/SwaggerClient-php/lib/",
          "BigCommerce\\OrdersV3\\" : "./clients/OrdersV3/SwaggerClient-php/lib/",
          "BigCommerce\\PriceLists\\" : "./clients/PriceLists/SwaggerClient-php/lib/",
          "BigCommerce\\Scripts\\" : "./clients/Scripts/SwaggerClient-php/lib/",
          "BigCommerce\\Sites\\" : "./clients/Sites/SwaggerClient-php/lib/",
          "BigCommerce\\StoreInfo\\" : "./clients/StoreInfo/SwaggerClient-php/lib/",
          "BigCommerce\\Subscribers\\" : "./clients/Subscribers/SwaggerClient-php/lib/",
          "BigCommerce\\Wishlists\\" : "./clients/Wishlists/SwaggerClient-php/lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "BigCommerce\\Widgets\\" : "./clients/Widgets/SwaggerClient-php/test/",
          "BigCommerce\\Themes\\" : "./clients/Themes/SwaggerClient-php/test/",
          "BigCommerce\\Catalog\\" : "./clients/Catalog/SwaggerClient-php/test/",
          "BigCommerce\\OrdersV2\\" : "./clients/OrdersV2/SwaggerClient-php/test/",
          "BigCommerce\\OrdersV3\\" : "./clients/OrdersV3/SwaggerClient-php/test/",
          "BigCommerce\\PriceLists\\" : "./clients/PriceLists/SwaggerClient-php/test/",
          "BigCommerce\\Scripts\\" : "./clients/Scripts/SwaggerClient-php/test/",
          "BigCommerce\\Sites\\" : "./clients/Sites/SwaggerClient-php/test/",
          "BigCommerce\\StoreInfo\\" : "./clients/StoreInfo/SwaggerClient-php/test/",
          "BigCommerce\\Subscribers\\" : "./clients/Subscribers/SwaggerClient-php/test/",
          "BigCommerce\\Wishlists\\" : "./clients/Wishlists/SwaggerClient-php/test/"
        }
    }
}
